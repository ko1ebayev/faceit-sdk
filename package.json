{
  "name": "faceit-sdk",
  "version": "1.0.1",
  "description": "TypeScript SDK for Faceit API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./players": {
      "import": "./dist/players/index.js",
      "types": "./dist/players/index.d.ts"
    },
    "./matches": {
      "import": "./dist/matches/index.js",
      "types": "./dist/matches/index.d.ts"
    },
    "./games": {
      "import": "./dist/games/index.js",
      "types": "./dist/games/index.d.ts"
    },
    "./matchmakings": {
      "import": "./dist/matchmakings/index.js",
      "types": "./dist/matchmakings/index.d.ts"
    },
    "./hubs": {
      "import": "./dist/hubs/index.js",
      "types": "./dist/hubs/index.d.ts"
    },
    "./championships": {
      "import": "./dist/championships/index.js",
      "types": "./dist/championships/index.d.ts"
    },
    "./leaderboards": {
      "import": "./dist/leaderboards/index.js",
      "types": "./dist/leaderboards/index.d.ts"
    },
    "./leagues": {
      "import": "./dist/leagues/index.js",
      "types": "./dist/leagues/index.d.ts"
    },
    "./organizers": {
      "import": "./dist/organizers/index.js",
      "types": "./dist/organizers/index.d.ts"
    },
    "./rankings": {
      "import": "./dist/rankings/index.js",
      "types": "./dist/rankings/index.d.ts"
    },
    "./search": {
      "import": "./dist/search/index.js",
      "types": "./dist/search/index.d.ts"
    },
    "./teams": {
      "import": "./dist/teams/index.js",
      "types": "./dist/teams/index.d.ts"
    },
    "./tournaments": {
      "import": "./dist/tournaments/index.js",
      "types": "./dist/tournaments/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "version": "npm run build && git add -A",
    "postversion": "git push && git push --tags",
    "release:patch": "npm version patch",
    "release:minor": "npm version minor",
    "release:major": "npm version major",
    "release:prerelease": "npm version prerelease --preid=beta"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/faceit-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/faceit-sdk/issues"
  },
  "homepage": "https://github.com/yourusername/faceit-sdk#readme",
  "keywords": [
    "faceit",
    "sdk",
    "typescript",
    "api",
    "esports",
    "gaming",
    "csgo",
    "cs2"
  ],
  "author": "ko1ebayev.worx@gmail.com",
  "license": "MIT",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "typescript": "^5.8.3"
  }
}
